<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Document</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL"
      crossorigin="anonymous"
    ></script>
  </head>
  <body>
    <table class="table">
      <thead>
        <tr>
          <th scope="col">id</th>
          <th scope="col" style="text-transform: capitalize">
            exchange rate subscriptions
          </th>
        </tr>
      </thead>
      <tbody>
        <% userExchangeRateSubs.forEach(function(entry) { %>
        <tr>
          <th scope="row"><%= userId %></th>
          <td><%= entry.from_to %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>

    <table class="table">
      <thead>
        <tr>
          <th scope="col">id</th>
          <th scope="col" style="text-transform: capitalize">
            mensa menu subscriptions
          </th>
        </tr>
      </thead>
      <tbody>
        <% userMensaMenuSubs.forEach(function(entry) { %>
        <tr>
          <th scope="row"><%= userId %></th>
          <td><%= entry.mensa_id %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
    <a href="/admin/users" class="btn btn-primary link-underline-opacity-0"
      >back to users</a
    >
  </body>
</html>
